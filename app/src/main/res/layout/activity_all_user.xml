<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_blue"
    tools:context="com.hiro.pchen.firebasechat.AccountModel.AllUserActivity">

    <include
        layout="@layout/bar_line"
        android:id="@+id/all_user_tool" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/all_user_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/all_user_tool"></android.support.v7.widget.RecyclerView>



</RelativeLayout>
